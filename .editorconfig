; This file is for unifying the coding style for different editors and IDEs.
; More information at http://EditorConfig.org

root = true

[*]
end_of_line = CRLF
insert_final_newline = true

[*.{cs,ps1}]
indent_style = space
indent_size = 4

[*.{xml,csproj,xslt}]
indent_style = space
indent_size = 2

[*.xml]
insert_final_newline = false
